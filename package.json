{
  "name": "@wonderflow/react-components",
  "description": "UI components from Wonderflow's Wanda design system",
  "version": "3.0.0-beta.3",
  "author": "Wonderflow",
  "license": "MIT",
  "main": "index.js",
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=14.x",
    "npm": ">=8.x"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wonderflow-bv/react-components.git"
  },
  "keywords": [
    "design-system",
    "wonderflow",
    "components",
    "react"
  ],
  "bugs": {
    "url": "https://github.com/wonderflow-bv/react-components/issues"
  },
  "homepage": "https://github.com/wonderflow-bv/react-components#readme",
  "browserslist": [
    "last 2 Chrome versions",
    "last 2 Firefox versions",
    "last 2 Safari versions",
    "last 2 Edge versions"
  ],
  "scripts": {
    "start": "run-s build:themes build:icons storybook",
    "storybook": "start-storybook --quiet -p 6006",
    "lint": "run-s lint:scripts && run-s lint:styles",
    "lint:scripts": "eslint \"src/**/*.{tsx,ts}\"",
    "lint:styles": "stylelint src/**/*.css",
    "lint:commits": "commitlint --edit",
    "prepare": "is-ci || husky install",
    "release": "semantic-release",
    "update:cssdb": "npx browserslist@latest --update-db",
    "update:deps": "npx npm-check -u",
    "prebuild": "rimraf dist",
    "copy:css": "copyfiles -u 1 \"src/**/*.css\" \"src/*.css\" dist/",
    "copy:icons": "copyfiles -u 1 \"src/icons/svgs/**/*.svg\" dist/",
    "optimize:svg": "svgo ./dist/icons/svgs/**/* --config=src/icons/svgo.config.js --quiet",
    "generate:sprites": "node src/icons/sprites.js",
    "prepare:themes": "node src/themes/combine.js",
    "prepare:package": "copyfiles package.json dist",
    "build:components": "tsc",
    "build:components:nocomments": "tsc -p ./tsconfig.nocomments.json",
    "build:css:core": "postcss dist/core/*.css dist/*.css --replace",
    "build:css:components": "postcss dist/**/*.module.css dist/**/**/*.module.css --replace",
    "build:css": "run-s build:csscore build:csscomponents",
    "build:themes": "run-p build:light build:dark && run-s prepare:themes",
    "build:light": "node src/themes/index.js --name light",
    "build:dark": "node src/themes/index.js --name dark",
    "build:icons": "run-s copy:icons optimize:svg generate:sprites",
    "build": "run-s build:icons build:components build:components:nocomments copy:css build:themes build:css:components build:css:core",
    "postbuild": "run-s prepare:package"
  },
  "dependencies": {
    "@react-aria/interactions": "3.7.0",
    "@wonderflow/tokens": "1.3.7",
    "ahooks": "3.0.8",
    "clsx": "1.1.1",
    "framer-motion": "4.1.17",
    "postcss": "^8.4.5",
    "react": "^17.0",
    "react-data-table-component": "7.4.5",
    "react-dom": "^17.0",
    "react-focus-on": "3.5.4",
    "react-loading-skeleton": "3.0.1",
    "react-masonry-css": "1.0.16",
    "react-paginate": "8.1.0",
    "react-popper-tooltip": "4.3.1",
    "react-roving-tabindex": "3.1.1",
    "react-uid": "2.3.1",
    "storybook-addon-turbo-build": "1.0.1",
    "type-fest": "2.8.0"
  },
  "peerDependencies": {
    "postcss": "^8.4.5",
    "react": "^17.0",
    "react-dom": "^17.0"
  },
  "devDependencies": {
    "@babel/core": "7.16.5",
    "@commitlint/cli": "15.0.0",
    "@commitlint/config-conventional": "15.0.0",
    "@semantic-release/exec": "6.0.3",
    "@semantic-release/git": "10.0.1",
    "@semantic-release/github": "8.0.2",
    "@semantic-release/npm": "8.0.3",
    "@semantic-release/release-notes-generator": "10.0.3",
    "@storybook/addon-actions": "6.4.9",
    "@storybook/addon-essentials": "6.4.9",
    "@storybook/addon-links": "6.4.9",
    "@storybook/addon-postcss": "2.0.0",
    "@storybook/addons": "6.4.9",
    "@storybook/react": "6.4.9",
    "@storybook/theming": "6.4.9",
    "@types/react-dom": "17.0.11",
    "@typescript-eslint/eslint-plugin": "5.7.0",
    "@typescript-eslint/parser": "5.7.0",
    "@wonderflow/config": "3.0.3",
    "@wonderflow/eslint-config": "1.1.8",
    "@wonderflow/stylelint-config": "1.1.3",
    "babel-loader": "8.2.3",
    "commit-analyzer-fail-on-no-release": "1.0.1",
    "commitizen": "4.2.4",
    "copyfiles": "2.4.1",
    "cz-conventional-changelog": "3.3.0",
    "directory-tree": "3.0.1",
    "eslint": "7.32.0",
    "eslint-plugin-react": "7.27.1",
    "fs-extra": "10.0.0",
    "husky": "7.0.4",
    "is-ci": "3.0.1",
    "nanospinner": "0.6.0",
    "npm-run-all": "4.1.5",
    "picocolors": "1.0.0",
    "postcss-cli": "9.1.0",
    "postcss-mixins": "9.0.1",
    "rimraf": "3.0.2",
    "semantic-release": "18.0.1",
    "storybook-addon-themes": "6.1.0",
    "storybook-css-modules-preset": "1.1.1",
    "style-dictionary": "3.0.3",
    "stylelint": "14.1.0",
    "svgo": "2.8.0",
    "svgstore": "3.0.1",
    "typescript": "4.5.4",
    "typescript-plugin-css-modules": "3.4.0",
    "yargs": "17.3.0"
  },
  "resolutions": {
    "rc": "1.2.8"
  }
}
