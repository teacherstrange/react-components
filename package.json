{
  "name": "@wonderflow/react-components",
  "description": "UI components from Wonderflow's Wanda design system",
  "version": "1.2.0",
  "author": "Wonderflow",
  "license": "MIT",
  "main": "index.js",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wonderflow-bv/react-components.git"
  },
  "keywords": [
    "design-system",
    "wonderflow",
    "components",
    "react"
  ],
  "bugs": {
    "url": "https://github.com/wonderflow-bv/react-components/issues"
  },
  "homepage": "https://github.com/wonderflow-bv/react-components#readme",
  "browserslist": [
    "last 2 Chrome versions",
    "last 2 Firefox versions",
    "last 1 Safari versions",
    "last 2 Edge versions"
  ],
  "scripts": {
    "start": "run-s build storybook",
    "storybook": "start-storybook --quiet -p 6006",
    "lint": "run-s lint:scripts && run-s lint:styles",
    "lint:scripts": "eslint \"src/**/*.{tsx,ts}\"",
    "lint:styles": "stylelint src/**/*.css",
    "lint:commits": "commitlint --edit",
    "prepare": "is-ci || husky install",
    "release": "semantic-release",
    "update:cssdb": "npx browserslist@latest --update-db",
    "prebuild": "rimraf dist",
    "copy:css": "copyfiles -u 1 src/**/*.css dist/",
    "build": "run-s -s build:components copy:css",
    "build:components": "tsc",
    "postbuild": "copyfiles package.json dist"
  },
  "dependencies": {
    "@wonderflow/config": "1.3.4",
    "@wonderflow/eslint-config": "1.0.1",
    "@wonderflow/stylelint-config": "1.0.1",
    "clsx": "1.1.1",
    "flat": "5.0.2",
    "postcss": "8.3.5",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "@babel/core": "7.14.6",
    "@commitlint/cli": "12.1.4",
    "@commitlint/config-conventional": "12.1.4",
    "@semantic-release/commit-analyzer": "8.0.1",
    "@semantic-release/exec": "5.0.0",
    "@semantic-release/git": "9.0.0",
    "@semantic-release/github": "7.2.3",
    "@semantic-release/npm": "7.1.3",
    "@semantic-release/release-notes-generator": "9.0.3",
    "@storybook/addon-actions": "6.3.4",
    "@storybook/addon-essentials": "6.3.4",
    "@storybook/addon-links": "6.3.4",
    "@storybook/addon-postcss": "2.0.0",
    "@storybook/react": "6.3.4",
    "@types/flat": "5.0.2",
    "@types/react-dom": "17.0.8",
    "@typescript-eslint/eslint-plugin": "4.28.2",
    "@typescript-eslint/parser": "4.28.2",
    "babel-loader": "8.2.2",
    "commitizen": "4.2.4",
    "copyfiles": "2.4.1",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "7.30.0",
    "eslint-config-standard": "16.0.3",
    "eslint-plugin-import": "2.23.4",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "5.1.0",
    "eslint-plugin-react": "7.24.0",
    "husky": "7.0.1",
    "is-ci": "2.0.0",
    "npm-run-all": "4.1.5",
    "rimraf": "3.0.2",
    "semantic-release": "17.4.4",
    "storybook-css-modules-preset": "1.1.1",
    "stylelint": "13.13.1",
    "typescript": "4.3.5",
    "typescript-plugin-css-modules": "3.4.0",
    "what-input": "5.2.10"
  }
}
