{
  "name": "@wonderflow/react-components",
  "description": "UI components from Wonderflow's Wanda design system",
  "version": "2.17.0",
  "author": "Wonderflow",
  "license": "MIT",
  "main": "index.js",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wonderflow-bv/react-components.git"
  },
  "keywords": [
    "design-system",
    "wonderflow",
    "components",
    "react"
  ],
  "bugs": {
    "url": "https://github.com/wonderflow-bv/react-components/issues"
  },
  "homepage": "https://github.com/wonderflow-bv/react-components#readme",
  "browserslist": [
    "last 2 Chrome versions",
    "last 2 Firefox versions",
    "last 1 Safari versions",
    "last 2 Edge versions"
  ],
  "scripts": {
    "start": "run-s build storybook",
    "storybook": "start-storybook --quiet -p 6006",
    "lint": "run-s lint:scripts && run-s lint:styles",
    "lint:scripts": "eslint \"src/**/*.{tsx,ts}\"",
    "lint:styles": "stylelint src/**/*.css",
    "lint:commits": "commitlint --edit",
    "prepare": "is-ci || husky install",
    "release": "semantic-release",
    "update:cssdb": "npx browserslist@latest --update-db",
    "update:deps": "npx npm-check -u",
    "prebuild": "rimraf dist",
    "copy:css": "copyfiles -u 1 \"src/**/*.css\" \"src/*.css\" dist/",
    "optimize:svg": "svgo ./src/icons/svgs/**/* --config=src/icons/svgo.config.js --quiet",
    "generate:sprites": "node src/icons/sprites.js",
    "build": "run-s build:icons build:components copy:css build:themes",
    "build:components": "tsc",
    "build:themes": "run-p build:light build:dark && run-s prepare:themes",
    "build:light": "node src/themes/index.js --name light",
    "build:dark": "node src/themes/index.js --name dark",
    "prepare:themes": "node src/themes/combine.js",
    "build:icons": "run-s optimize:svg generate:sprites",
    "postbuild": "copyfiles package.json dist"
  },
  "dependencies": {
    "@bumaga/tabs": "0.2.0",
    "@radix-ui/react-polymorphic": "0.0.13",
    "@react-aria/interactions": "3.5.1",
    "@wonderflow/config": "1.4.7",
    "@wonderflow/eslint-config": "1.1.0",
    "@wonderflow/stylelint-config": "1.0.1",
    "@wonderflow/tokens": "1.3.3",
    "clsx": "1.1.1",
    "flat": "5.0.2",
    "framer-motion": "4.1.17",
    "postcss": "8.3.6",
    "react": ">=17.0.2",
    "react-dom": ">=17.0.2",
    "react-popper-tooltip": "4.3.0",
    "react-roving-tabindex": "3.1.1",
    "react-uid": "2.3.1",
    "react-use": "17.3.1",
    "style-dictionary": "3.0.2",
    "type-fest": "2.1.0"
  },
  "peerDependencies": {
    "react": ">=17.0.2",
    "react-dom": ">=17.0.2"
  },
  "devDependencies": {
    "@babel/core": "7.15.0",
    "@commitlint/cli": "13.1.0",
    "@commitlint/config-conventional": "13.1.0",
    "@semantic-release/exec": "5.0.0",
    "@semantic-release/git": "9.0.0",
    "@semantic-release/github": "7.2.3",
    "@semantic-release/npm": "7.1.3",
    "@semantic-release/release-notes-generator": "9.0.3",
    "@storybook/addon-actions": "6.3.8",
    "@storybook/addon-essentials": "6.3.8",
    "@storybook/addon-links": "6.3.8",
    "@storybook/addon-postcss": "2.0.0",
    "@storybook/addons": "6.3.8",
    "@storybook/react": "6.3.8",
    "@storybook/theming": "6.3.8",
    "@types/flat": "5.0.2",
    "@types/react-dom": "17.0.9",
    "@typescript-eslint/eslint-plugin": "4.29.3",
    "@typescript-eslint/parser": "4.29.3",
    "babel-loader": "8.2.2",
    "commit-analyzer-fail-on-no-release": "1.0.0",
    "commitizen": "4.2.4",
    "copyfiles": "2.4.1",
    "cz-conventional-changelog": "3.3.0",
    "directory-tree": "2.3.1",
    "eslint": "7.32.0",
    "eslint-config-standard": "16.0.3",
    "eslint-plugin-import": "2.24.2",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "5.1.0",
    "eslint-plugin-react": "7.25.1",
    "fs-extra": "10.0.0",
    "husky": "7.0.2",
    "is-ci": "3.0.0",
    "npm-run-all": "4.1.5",
    "ora": "5.4.1",
    "rimraf": "3.0.2",
    "semantic-release": "17.4.7",
    "storybook-addon-themes": "6.1.0",
    "storybook-css-modules-preset": "1.1.1",
    "stylelint": "13.13.1",
    "svgo": "2.5.0",
    "svgstore": "3.0.0-2",
    "typescript": "4.3.5",
    "typescript-plugin-css-modules": "3.4.0",
    "yargs": "17.1.1"
  }
}
